{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noEmitOnError": true
  }
}
